import java.util.Arrays;

public class Vetor90 {

    public static void Ordem() {

        int[] v = new int[]{5, 1, 4, 3, 2};
        
        for (int i = 0; i < v.length; i++) {
            Arrays.sort(v);
            System.out.println(v[i]);

        }
    }

    public static void main(String[] args) {
        Ordem();
    }
}
